{
  "name": "ssh-mini-manager",
  "author": "mini",
  "private": true,
  "version": "0.0.1",
  "main": "app/electron/main.js",
  "build": {
    "productName": "ssh-mini-manager",
    "copyright": "Copyright Â© 2022 ${author}",
    "files": [
      "!node_modules/**/*",
      "package.json",
      {
        "from": "dist/vue",
        "to": "app/vue",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "app/electron",
        "to": "app/electron",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist/electron/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "wait-on tcp:3000 && cross-env ELECTRON_ENV=dev electron .",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"yarn electron\"",
    "electron:build": "cross-env ELECTRON=true vite build && electron-builder"
  },
  "dependencies": {
    "electron-store": "^8.0.1",
    "ssh-config": "^4.1.4",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "vite": "^2.9.5",
    "wait-on": "^6.0.1"
  }
}
